<a class="icon-with-text-container" href="#modal_schedule">
  <%= icon_tag("star", ["icon-part"]) %>
  <div class="text-part">
    <%= t("listings.edit_links.listing_schedule") %>
  </div>
</a>

<div class="remodal" data-remodal-id="modal_schedule">
  <button data-remodal-action="close" class="remodal-close"></button>
  <h1>Schedule Of the listing</h1>
  <div id="calendar"></div>
</div>
<script src="//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
<script src='http://fullcalendar.io/js/fullcalendar-2.1.1/lib/moment.min.js'></script>
<!--<script src='http://fullcalendar.io/js/fullcalendar-2.1.1/lib/jquery.min.js'></script>-->
<script src="http://fullcalendar.io/js/fullcalendar-2.1.1/lib/jquery-ui.custom.min.js"></script>
<script src='http://fullcalendar.io/js/fullcalendar-2.1.1/fullcalendar.min.js'></script>
<script>
    $(document).ready(function() {
        $('#calendar').fullCalendar({
            defaultDate: '2016-09-12',
            editable: true,
            eventLimit: true, // allow "more" link when too many events
            events: <%= raw @events.to_json %>,
        });
    });
</script>
